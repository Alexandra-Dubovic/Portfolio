						"exec": [
							"\r",
							"\r",
							"pm.test(\"Verify that for all activitie location is Spain\"), function(){\r",
							"pm.expect(Location_is_Spain).to.be.true; \r",
							"\r",
							"Location_is_Spain=true;\r",
							"for(var i = 0; i<=pm.response.json()[0].length;i++)\r",
							"{\r",
							"if(pm.response.json()[0][i][\"location_country\"]!=\"Spain\"){\r",
