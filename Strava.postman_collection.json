		{
			"name": "Create new activity",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Verify that response code is 200OK\"), function(){pm.response.to.have.status(200);}\r",
							"\r",
							"\r",
							"\r",
							"pm.test(\"Verify that response is obtained in less than 200 ms\", function(){\r",
							"pm.expect(pm.response.responseTime).to.be.lessThan(200);\r",
							"})\r",
							"pm.test(\"Body contains string sport type\",() => {\r",
							"  pm.expect(pm.response.text()).to.include(\"sport_type\");\r",
							"})\r",
							"\r",
							"const jsonData = pm.response.json();\r",
							"pm.test(\"Test data type of the response\", () => {\r",
							"  pm.expect(jsonData.name).to.be.a(\"string\");\r",
							"  pm.expect(jsonData.distance).to.be.a(\"number\");\r",
							"  pm.expect(jsonData.location_city).to.be.null;\r",
							"});\r",
							"\r",
							"pm.test(\"Value is in valid list\", () => {\r",
							"  pm.expect(pm.response.json().type)\r",
							"    .to.be.oneOf([\"Run\", \"Alpine ski\", \"Hike\"]);\r",
							"});\r",
							"\r",
							"pm.test(\"Person is from Spain\", () => {\r",
							"  const responseJson = pm.response.json();\r",
							"  pm.expect(responseJson.location_country).to.eql(\"Spain\");\r",
							"});\r",
							"\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
